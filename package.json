{
  "name": "react-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts-ts": "2.7.0",
    "rxjs": "^5.5.2",
    "ws": "^3.2.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "startSock": "tsc-watch ./src/sockServer.tsx --jsx react --lib 'es6,dom' --outDir ./dist --onSuccess 'node ./dist/sockServer.js'",
    "both": "concurrently \"react-scripts-ts start\" \"npm run startSock\""
  },
  "devDependencies": {
    "@types/jest": "^21.1.2",
    "@types/node": "^8.0.33",
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/ws": "^3.2.0",
    "concurrently": "^3.5.0",
    "tsc-watch": "^1.0.8"
  }
}
